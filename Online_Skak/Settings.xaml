<Page x:Class="Online_Skak.Settings"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Online_Skak"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Settings">


    <Grid Name="settings" Background="White">
        <Frame x:Name="Head" NavigationUIVisibility="Hidden" Margin="0,0,-0.4,0">
            <Frame.Background>
                <ImageBrush ImageSource="Background/Baggrund2.jpg"/>
            </Frame.Background>
        </Frame>
        <Label Content="Indstillinger" HorizontalAlignment="Left" Margin="310,28,0,0" VerticalAlignment="Top" FontSize="36" Background="{x:Null}" Foreground="White" FontFamily="Yu Gothic UI Light" FontWeight="Bold" RenderTransformOrigin="0.496,0.512"/>
        <Button Content="Tilbage" FontSize="20" HorizontalAlignment="Left" Margin="668,385,0,0" VerticalAlignment="Top" Width="103" Click="Button_Click" Height="35" FontFamily="Yu Gothic UI Light"/>
    </Grid>
</Page>
